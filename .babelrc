{
    "presets": [
        [
            "es2015",
            {
                "modules": false,
                "loose": true
            }
        ],
        "react"
    ],
    "plugins": ["transform-object-rest-spread"],
    "env": {
        "test": {
            "presets": [["es2015"], "react"]
        },
        "production": {
            "plugins": [
                "external-helpers",
                ["transform-react-remove-prop-types", { "removeImport": true }]
            ]
        }
    }
}
