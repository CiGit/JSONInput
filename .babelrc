{
    "presets": [
        [
            "es2015",
            {
                "modules": false,
                "loose": true
            }
        ],
        "react"
    ],
    "plugins": [
        "transform-object-rest-spread"
    ],
    "env": {
        "development": {
        },
        "production": {
            "plugins": [
                "external-helpers",
                [
                    "transform-react-remove-prop-types",
                    { "removeImport": true }
                ]
            ]
        }
    }
}
